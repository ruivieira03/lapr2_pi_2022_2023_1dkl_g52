<AnchorPane prefHeight="486.0" prefWidth="601.0" xmlns="http://javafx.com/javafx/20.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="pt.ipp.isep.dei.esoft.project.ui.gui.ReviewVisitRequestsGUI">
   <ListView fx:id="ListView" layoutX="36.0" layoutY="115.0" prefHeight="272.0" prefWidth="529.0" />
