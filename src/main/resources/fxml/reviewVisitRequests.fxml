      <font>
         <Font size="33.0" />
      </font>
   </Label>
   <Button fx:id="acceptButton" layoutX="35.0" layoutY="411.0" mnemonicParsing="false" onAction="#acceptVisitRequest" prefHeight="42.0" prefWidth="202.0" text="Accept">
      <font>
         <Font size="18.0" />
      </font>
   </Button>
   <Button fx:id="declineButton" layoutX="363.0" layoutY="411.0" mnemonicParsing="false" onAction="#declineVisitRequest" prefHeight="42.0" prefWidth="202.0" text="Decline">
      <font>
         <Font size="18.0" />
      </font>
   </Button>
   <MenuBar layoutX="507.0" layoutY="14.0" prefHeight="26.0" prefWidth="79.0">
      <Menu mnemonicParsing="false" text="Account">
         <MenuItem fx:id="logoutButton" mnemonicParsing="false" onAction="#logout" text="Logout" />
         <MenuItem fx:id="agentMenu" mnemonicParsing="false" onAction="#back" text="Agent menu" />
      </Menu>
   </MenuBar>
   <Button layoutX="261.0" layoutY="411.0" mnemonicParsing="false" onAction="#sortList" prefHeight="42.0" prefWidth="79.0" text="Sort" />
